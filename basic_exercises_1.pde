void setup(){
  size(1200, 600);
  fill(141, 201, 224);
  strokeWeight(5);
  strokeJoin(ROUND);
  
  // letter H
  rect(50, 100, 50, 300);
  rect(200, 100, 50, 300);
  rect(100, 225, 100, 50);
  
  // letter E
  rect(300, 100, 50, 300);
  rect(300, 100, 150, 50);
  rect(300, 350, 150, 50);
  rect(300, 225, 150, 50);
  
  // letter L (x2)
  rect(500, 100, 50, 300);
  rect(500, 350, 150, 50);
  
  rect(700, 100, 50, 300);
  rect(700, 350, 150, 50);
  
  // letter O
  rect(900, 100, 50, 300);
  rect(1050, 100, 50, 300);
  rect(950, 100, 100, 50);
  rect(950, 350, 100, 50);
}
